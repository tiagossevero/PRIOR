    "## üéØ 6. FUN√á√ÉO PRINCIPAL - GERAR SCHEMA"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "funcao-principal",
   "metadata": {},
   "outputs": [],
   "source": [
    "def gerar_schema_tabela(tabela):\n",
    "    \"\"\"\n",
    "    Gera schema completo de uma tabela\n",
    "    \n",
    "    Args:\n",
    "        tabela: nome da tabela\n",
    "    \n",
    "    Returns:\n",
    "        dict com metadata da tabela\n",
    "    \"\"\"\n",
    "    print(f\"\\n{'='*60}\")\n",
    "    print(f\"üìä Processando: {DATABASE}.{tabela}\")\n",
    "    print(f\"{'='*60}\")\n",
    "    \n",
    "    resultado = {\n",
    "        'database': DATABASE,\n",
    "        'tabela': tabela,\n",
    "        'timestamp': datetime.now().isoformat(),\n",
    "        'describe_formatted': None,\n",
    "        'sample_data': None,\n",
    "        'row_count': None,\n",
    "        'arquivos_gerados': []\n",
    "    }\n",
    "    \n",
    "    try:\n",
    "        # 1. DESCRIBE FORMATTED\n",
    "        print(f\"\\nüîç Executando: DESCRIBE FORMATTED {DATABASE}.{tabela}\")\n",
    "        df_describe = spark.sql(f\"DESCRIBE FORMATTED {DATABASE}.{tabela}\").toPandas()\n",
    "        resultado['describe_formatted'] = df_describe.to_dict('records')\n",
    "        \n",
    "        # Salvar describe\n",
    "        arquivo_csv = salvar_dataframe_como_csv(df_describe, f\"{tabela}_describe_formatted.csv\")\n",
    "        arquivo_json = salvar_dataframe_como_json(df_describe, f\"{tabela}_describe_formatted.json\")\n",
    "        arquivo_md = salvar_dataframe_como_markdown(\n",
    "            df_describe,\n",
    "            f\"{tabela}_describe_formatted.md\",\n",
    "            f\"DESCRIBE FORMATTED {DATABASE}.{tabela}\"\n",
    "        )\n",
    "        \n",
    "        resultado['arquivos_gerados'].extend([arquivo_csv, arquivo_json, arquivo_md])\n",
    "        \n",
    "        print(f\"   ‚úÖ Schema obtido: {len(df_describe)} linhas\")\n",
    "        print(f\"   üíæ Salvos: CSV, JSON, Markdown\")\n",
    "        \n",
    "        # 2. SELECT * FROM ... LIMIT N\n",
    "        print(f\"\\nüìã Executando: SELECT * FROM {DATABASE}.{tabela} LIMIT {SAMPLE_LIMIT}\")\n",
    "        df_sample = spark.sql(f\"SELECT * FROM {DATABASE}.{tabela} LIMIT {SAMPLE_LIMIT}\").toPandas()\n",
    "        resultado['sample_data'] = df_sample.to_dict('records')\n",
    "        \n",
    "        # Salvar sample\n",
    "        arquivo_csv = salvar_dataframe_como_csv(df_sample, f\"{tabela}_sample_{SAMPLE_LIMIT}.csv\")\n",
    "        arquivo_json = salvar_dataframe_como_json(df_sample, f\"{tabela}_sample_{SAMPLE_LIMIT}.json\")\n",
    "        arquivo_md = salvar_dataframe_como_markdown(\n",
    "            df_sample,\n",
    "            f\"{tabela}_sample_{SAMPLE_LIMIT}.md\",\n",
    "            f\"SELECT * FROM {DATABASE}.{tabela} LIMIT {SAMPLE_LIMIT}\"\n",
    "        )\n",
    "        \n",
    "        resultado['arquivos_gerados'].extend([arquivo_csv, arquivo_json, arquivo_md])\n",
    "        \n",
    "        # IMPORTANTE: Usar len() explicitamente para evitar conflito com sum() do PySpark\n",
    "        num_colunas = len(df_sample.columns)\n",
    "        \n",
    "        print(f\"   ‚úÖ Sample obtido: {len(df_sample)} registros x {num_colunas} colunas\")\n",
    "        print(f\"   üíæ Salvos: CSV, JSON, Markdown\")\n",
    "        \n",
    "        # 3. COUNT(*)\n",
    "        print(f\"\\nüî¢ Executando: SELECT COUNT(*) FROM {DATABASE}.{tabela}\")\n",
    "        result = spark.sql(f\"SELECT COUNT(*) as total FROM {DATABASE}.{tabela}\").collect()\n",
    "        total_rows = result[0]['total']\n",
    "        resultado['row_count'] = int(total_rows)\n",
    "        \n",
    "        print(f\"   ‚úÖ Total de registros: {total_rows:,}\")\n",
    "        \n",
    "        # 4. Resumo da tabela\n",
    "        print(f\"\\nüìä Resumo da tabela {tabela}:\")\n",
    "        print(f\"   - Total de registros: {total_rows:,}\")\n",
    "        print(f\"   - Total de colunas: {num_colunas}\")\n",
    "        \n",
    "        # Mostrar primeiras 10 colunas\n",
    "        colunas_lista = df_sample.columns.tolist()\n",
    "        primeiras_colunas = colunas_lista[:10]\n",
    "        tem_mais = len(colunas_lista) > 10\n",
    "        \n",
    "        colunas_str = ', '.join(primeiras_colunas)\n",
    "        if tem_mais:\n",
    "            colunas_str += '...'\n",
    "        \n",
    "        print(f\"   - Colunas: {colunas_str}\")\n",
    "        \n",
    "        # IMPORTANTE: Usar len() para contar arquivos, n√£o sum()\n",
    "        print(f\"   - Arquivos gerados: {len(resultado['arquivos_gerados'])}\")\n",
    "        \n",
    "        return resultado\n",
    "        \n",
    "    except Exception as e:\n",
    "        print(f\"   ‚ùå Erro ao processar {tabela}: {str(e)}\")\n",
    "        resultado['error'] = str(e)\n",
    "        return resultado\n",
    "\n",
    "print(\"‚úÖ Fun√ß√£o principal definida\")"
   ]
