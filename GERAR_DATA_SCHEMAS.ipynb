   "source": [
    "print(\"=\"*60)\n",
    "print(\"üöÄ GERADOR DE DATA SCHEMAS - GECOB\")\n",
    "print(\"=\"*60)\n",
    "print(f\"Database: {DATABASE}\")\n",
    "print(f\"Tabelas: {len(TABELAS)}\")\n",
    "print(f\"Output: ./{OUTPUT_DIR}/\")\n",
    "print(f\"Sample limit: {SAMPLE_LIMIT} registros\")\n",
    "print(\"=\"*60)\n",
    "\n",
    "# Criar diret√≥rio de sa√≠da\n",
    "criar_diretorio_output()\n",
    "\n",
    "# Processar cada tabela\n",
    "resultados = []\n",
    "inicio_total = datetime.now()\n",
    "\n",
    "for i, tabela in enumerate(TABELAS, 1):\n",
    "    print(f\"\\n\\n[{i}/{len(TABELAS)}] Processando tabela: {tabela}\")\n",
    "    inicio = datetime.now()\n",
    "    \n",
    "    resultado = gerar_schema_tabela(tabela)\n",
    "    resultados.append(resultado)\n",
    "    \n",
    "    fim = datetime.now()\n",
    "    duracao = (fim - inicio).total_seconds()\n",
    "    print(f\"\\n‚è±Ô∏è  Tempo de processamento: {duracao:.2f}s\")\n",
